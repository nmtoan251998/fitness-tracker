{
  "name": "fitness-tracker",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "clean": "rimraf dist/*  ",
    "test": "mocha --exit src/api/tests/*.test.js --timeout 30000",
    "test-coverage": "nyc npm run test",
    "dev:client": "parcel src/views/*.html --no-source-maps --port 3000 --no-cache",
    "dev:server": "nodemon --inspect src/server",
    "dev": "concurrently -n \" Clean , Client , Server \" \"npm run clean\" \"npm run dev:client\" \"npm run dev:server\" ",
    "start-python": "python3 src/utils/miband2/sendall.py --live --mac CE:36:72:D8:4F:64",
    "build:client": "parcel build src/views/*.html --out-dir dist --no-cache",
    "build:server": "NODE_ENV=production babel src -s -d dist",
    "production": "concurrently -n \" Clean , Build-client , Build-server \" \"npm run clean\" \"npm run build:client\" \"npm run build:server\"",
    "start": "node bin/production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": [
    "last 1 Chrome version",
    "Firefox ESR",
    "last 1 safari version",
    "last 1 opera version",
    "last 1 edge version"
  ],
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "child_process": "^1.0.2",
    "concurrently": "^4.1.2",
    "cors": "^2.8.5",
    "cron": "^1.7.2",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "function.js": "^1.3.0",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "nexmo": "^2.4.2",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "puppeteer": "^1.20.0",
    "redis": "^2.8.0",
    "socket.io": "^2.2.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "babel-cli": "^6.26.0",
